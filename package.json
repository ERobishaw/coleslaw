{
  "name": "coleslaw",
  "version": "0.0.5",
  "description": "Strongly typed OData library written in TypeScript",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/angry-yard/coleslaw.git"
  },
  "author": {
    "name": "Ryan Grady",
    "email": "ryan@angry-yard.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/angry-yard/coleslaw/issues"
  },
  "homepage": "https://github.com/angry-yard/coleslaw#readme",
  "scripts": {
    "build": "npm run clearDist && npm run compile && npm run copyFiles",
    "copyFiles": "copy README.md dist && copy package.json dist",
    "clearDist": "del dist\\*.* /s /q",
    "publish": "npm publish ./dist",
    "compile": "tsc -p prod.tsconfig.json"
  },
  "dependencies": {
    "@angular/http": "4.0.1",
    "rxjs": "5.3.0"
  },
  "devDependencies": {
    "typescript": "2.2.2"
  }
}
